    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "dev",
                "--target",
                "lib/main_dev.dart"
            ]
        },
        {
            "name": "Flutter(No Flavor)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--target",
                "lib/main_dev.dart"
            ]
        },
        {
            "name": "Flutter Profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--flavor",
                "profile",
                "--target",
                "lib/main_profile.dart"
            ]
        },
        {
            "name": "Flutter Production",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "prod"
            ]
        },
    ]
}